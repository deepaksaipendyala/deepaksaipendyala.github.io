                <assign variable="t" expression="tdeg+tplas+telec"/>
                <assign variable="total" expression="total+t"/>
                <assign variable="final1" expression="final1+tdeg"/>
                <assign variable="final2" expression="final2+tplas"/>
                <assign variable="final3" expression="final3+telec"/>
                <assign variable="k" expression="0"/>
                <assign variable="i" expression="0"/>
                <assign variable="h" expression="x"/>
            </for>
            <assign variable="final1" expression="(final1/total)*100"/>
            <assign variable="final2" expression="(final2/total)*100"/>
            <assign variable="final3" expression="(final3/total)*100"/>
            <output expression="&quot;total wastage is &quot;&amp;total &amp; &quot; KGS&quot;" newline="True"/>
            <output expression="ToFixed(final1,2)&amp;&quot;%&quot;&amp;&quot; is from degradedable waste category&quot;" newline="True"/>
            <output expression="ToFixed(final2,2)&amp;&quot;%&quot;&amp;&quot; is from plastic waste category&quot;" newline="True"/>
            <output expression="ToFixed(final3,2)&amp;&quot;%&quot;&amp;&quot; is from electronic waste category&quot;" newline="True"/>
        </body>
    </function>
    <function name="totalearning" type="None" variable="">
